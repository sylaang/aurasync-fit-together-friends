{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      },
      {
        "src": "backend/backend/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(frontend|build|static|images|assets)(.*)",
        "dest": "/frontend$2"
      },
      {
        "src": "/(.*)",
        "dest": "backend/backend/wsgi.py"
      }
    ]
  }
  